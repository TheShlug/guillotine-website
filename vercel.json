{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1"
    },
    {
      "src": "/data/(.*)",
      "dest": "/frontend/data/$1"
    },
    {
      "src": "/rules",
      "dest": "/frontend/rules.html"
    },
    {
      "src": "/average-finishes",
      "dest": "/frontend/average-finishes.html"
    },
    {
      "src": "/draft-order",
      "dest": "/frontend/draft-order.html"
    },
    {
      "src": "/transactions",
      "dest": "/frontend/transactions.html"
    },
    {
      "src": "/death-bell",
      "dest": "/frontend/death-bell.html"
    },
    {
      "src": "/season-recap",
      "dest": "/frontend/season-recap.html"
    },
    {
      "src": "/manager",
      "dest": "/frontend/manager.html"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    }
  ]
}
